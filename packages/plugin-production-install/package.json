{
  "name": "@larry1123/yarn-plugin-production-install",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "scripts": {
    "pre-build": "yarn run clean",
    "build": "yarn run build:plugin-production-install",
    "build:plugin-production-install": "yarn run build:cjs && yarn run build:plugin",
    "clean": "yarn run clean:plugin-production-install",
    "clean:plugin-production-install": "rm -rf lib bundles",
    "format": "yarn run format:package.json && yarn run format:prettier && yarn run lint:eslint --fix",
    "pre-test": "rm -rf out/",
    "test": "yarn prod-install out",
    "post-test": "cp -R lib/. out/"
  },
  "dependencies": {
    "@yarnpkg/cli": "2.1.1",
    "@yarnpkg/core": "2.1.1",
    "@yarnpkg/fslib": "2.1.0",
    "@yarnpkg/plugin-pack": "2.1.0",
    "clipanion": "2.4.4"
  },
  "devDependencies": {
    "typescript-transform-paths": "^1.1.14"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "*",
    "@yarnpkg/core": "*",
    "@yarnpkg/fslib": "*",
    "@yarnpkg/plugin-pack": "*",
    "clipanion": "*"
  },
  "optionalDependencies": {
    "@types/node": "12.0.0"
  },
  "pluginEnabled": true
}
